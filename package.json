{
  "name": "aamva-parser",
  "version": "1.4.1",
  "description": "Parse AAMVA PDF417 barcode data from driver's licenses and ID cards",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joptimus/aamva-parser.git"
  },
  "keywords": [
    "aamva",
    "pdf417",
    "barcode",
    "driver-license",
    "drivers-license",
    "id-card",
    "identification",
    "parser",
    "scanner",
    "reader",
    "age-verification",
    "id-verification",
    "dmv",
    "real-id",
    "usa",
    "canada"
  ],
  "author": "joptimus",
  "license": "ISC",
  "homepage": "https://github.com/joptimus/aamva-parser#readme",
  "bugs": {
    "url": "https://github.com/joptimus/aamva-parser/issues"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "typescript": "^5.5.3"
  }
}
